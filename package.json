{
  "name": "heticiens.news",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "lint": "eslint --color --ignore-path .gitignore \"**/*.js\" \"**/*.ts\" \"**/*.tsx\"",
    "test": "tsc --noEmit",
    "start": "next",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "serve": "next start",
    "deploy": "vc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "komit $HUSKY_GIT_PARAMS",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts": "eslint --cache --fix",
    "*.tsx": "eslint --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hetic-newsroom/heticiens.news.git"
  },
  "author": "Gabriel SAILLARD <gabriel@saillard.dev> (https://gaby.dev)",
  "contributors": [
    "Arnaud MEGE <arnaud.mege@hetic.net>",
    "Joshua TABAKHOFF <joshua.tabakhoff@hetic.net>"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/hetic-newsroom/heticiens.news/issues"
  },
  "homepage": "https://github.com/hetic-newsroom/heticiens.news#readme",
  "dependencies": {
    "@prismicio/client": "^4.0.0",
    "classnames": "^2.2.6",
    "deepmerge": "^4.2.2",
    "feed": "^4.2.2",
    "framer-motion": "^3.3.0",
    "next": "^10.0.6",
    "next-seo": "^4.19.0",
    "prismic-reactjs": "^1.3.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-use": "^17.1.1",
    "reading-time": "^1.3.0",
    "url-parse": "^1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@next/bundle-analyzer": "^10.0.6",
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@vercel/node": "^1.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "husky": "^4.3.8",
    "komit": "^1.0.5",
    "lint-staged": "^10.5.4",
    "sass": "^1.32.7",
    "typescript": "^4.1.5",
    "vercel": "^21.2.3"
  }
}
