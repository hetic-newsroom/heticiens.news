{
  "name": "heticiens.news",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "lint": "eslint --color --ignore-path .gitignore \"**/*.js\" \"**/*.ts\" \"**/*.tsx\"",
    "test": "tsc --noEmit",
    "start": "next",
    "build": "next build",
    "serve": "next start",
    "deploy": "vc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "komit $HUSKY_GIT_PARAMS",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts": "eslint --cache --fix",
    "*.tsx": "eslint --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hetic-newsroom/heticiens.news.git"
  },
  "author": "Gabriel SAILLARD <gabriel@saillard.dev> (https://gaby.dev)",
  "contributors": [
    "Arnaud MEGE <arnaud.mege@hetic.net>",
    "Joshua TABAKHOFF <joshua.tabakhoff@hetic.net>"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/hetic-newsroom/heticiens.news/issues"
  },
  "homepage": "https://github.com/hetic-newsroom/heticiens.news#readme",
  "dependencies": {
    "@prismicio/client": "^4.0.0",
    "aws-sdk": "^2.829.0",
    "classnames": "^2.2.6",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^10.0.5",
    "prismic-reactjs": "^1.3.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reading-time": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vercel/node": "^1.9.0",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "komit": "^1.0.5",
    "lint-staged": "^10.5.3",
    "sass": "^1.32.5",
    "typescript": "^4.1.3",
    "vercel": "^21.1.0"
  }
}
