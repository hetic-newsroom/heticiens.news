{
  "name": "heticiens.news",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "start": "next",
    "build": "next build",
    "serve": "next start",
    "deploy": "serverless",
    "lint": "xo",
    "test": "echo 'no tests yet!'",
    "commit": "commit"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm run build"
    }
  },
  "xo": {
    "envs": [
      "browser",
      "node"
    ],
    "extends": "xo-react",
    "ignores": [
      "local/**",
      "components/head.js"
    ],
    "rules": {
      "react/state-in-constructor": [
        1,
        "always"
      ],
      "import/no-unassigned-import": 0,
      "react/react-in-jsx-scope": 0,
      "unicorn/prevent-abbreviations": 0,
      "unicorn/catch-error-name": 0,
      "unicorn/prefer-set-has": 0,
      "react/prop-types": 0,
      "react/button-has-type": 0,
      "no-await-in-loop": 0,
      "@typescript-eslint/prefer-readonly-parameter-types": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hetic-newsroom/heticiens.news.git"
  },
  "author": "Gabriel SAILLARD <gabriel@saillard.dev> (https://gaby.dev)",
  "contributors": [
    "Arnaud MEGE <arnaud.mege@hetic.net>",
    "Joshua TABAKHOFF <joshua.tabakhoff@hetic.net>"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/hetic-newsroom/heticiens.news/issues"
  },
  "homepage": "https://github.com/hetic-newsroom/heticiens.news#readme",
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "https://github.com/hetic-newsroom/ckeditor5-build-classic.git#master",
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "aws-sdk": "^2.668.0",
    "canvas": "^2.6.1",
    "classnames": "^2.2.6",
    "dompurify": "^2.0.10",
    "isomorphic-unfetch": "^3.0.0",
    "jsdom": "^16.2.2",
    "nanoid": "^3.1.3",
    "next": "^9.3.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reading-time": "^1.2.0",
    "scrypt-js": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt-cli": "^8.3.5",
    "@types/react": "^16.9.34",
    "eslint-config-xo": "^0.29.1",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.2.5",
    "serverless": "^1.69.0",
    "serverless-next.js": "^1.11.0",
    "xo": "^0.30.0"
  }
}
